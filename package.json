{
  "name": "unfisk",
  "version": "1.0.0",
  "description": "OADA uservice to &#34;unflatten&#34; a list into a list of links",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node index.ts",
    "build": "tsc",
    "fix": "prettier-standard **/*.ts **/*.js",
    "lint": "standard **/*.ts **/*.js"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "dependencies": {
    "@oada/oada-cache": "^3.0.1",
    "bluebird": "^3.7.2",
    "debug": "^4.1.1",
    "nconf": "^0.10.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/debug": "^4.1.5",
    "@types/nconf": "^0.10.0",
    "@types/node": "^13.5.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "prettier-standard": "^16.1.0",
    "standard": "^14.3.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
